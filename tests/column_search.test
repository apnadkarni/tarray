# Same as above but tests done with target column being a span column

namespace eval tarray::test {

    foreach type {any string boolean byte double int uint wide} {

        test column_search-span-$type-eq-1.0 {
            Search an empty column
        } -body {
            tarray::column search -eq [newspancolumn $type] [samplevalue $type]
        } -result -1

        test column_search-span-$type-eq-1.1 {
            Search an empty column -all
        } -body {
            tarray::column search -eq -all [newspancolumn $type] [samplevalue $type]
        } -result [crep int {}] -match column

        test column_search-span-$type-eq-1.2 {
            Search an empty column -inline
        } -body {
            tarray::column search -eq -inline [newspancolumn $type] [samplevalue $type]
        } -result {}

        test column_search-span-$type-eq-1.3 {
            Search an empty column -inline -all
        } -body {
            tarray::column search -eq -inline -all [newspancolumn $type] [samplevalue $type]
        } -result [crep $type {}] -match column

        test column_search-span-$type-eq-1.4 {
            Search an empty column -not
        } -body {
            tarray::column search -eq -not [newspancolumn $type] [samplevalue $type]
        } -result -1

        test column_search-span-$type-eq-1.5 {
            Search an empty column -not -inline
        } -body {
            tarray::column search -eq -not -inline [newspancolumn $type] [samplevalue $type]
        } -result {}

        test column_search-span-$type-eq-1.6 {
            Search an empty column -not -inline -all
        } -body {
            tarray::column search -eq -not -all -inline [newspancolumn $type] [samplevalue $type]
        } -result [crep $type {}] -match column


        test column_search-span-$type-eq-2.0 {
            Match on first element
        } -body {
            tarray::column search -eq [samplespancolumn $type] [samplevalue $type 0]
        } -result 0

        test column_search-span-$type-eq-2.1 {
            Match on last element
        } -body {
            # Note last element may occur multiple times
            tarray::column search -eq [samplespancolumn $type] [samplevalue $type end]
        } -result [lsearch -exact [samplerange $type] [samplevalue $type end]]

        test column_search-span-$type-eq-2.2 {
            Match -eq -inline
        } -body {
            # Note last element may occur multiple times
            tarray::column search -inline -eq [samplespancolumn $type] [samplevalue $type end]
        } -result [lsearch -exact -inline [samplerange $type] [samplevalue $type end]]

        test column_search-span-$type-eq-2.3 {
            Match -eq -all
        } -body {
            tarray::column search -all -eq [samplespancolumn $type] [samplevalue $type 0]
        } -result [crep int [lsearch -exact -all [samplerange $type] [samplevalue $type 0]]] -match column

        test column_search-span-$type-eq-2.4 {
            Match -eq -all (ensure duplicates)
        } -body {
            set val [samplevalue $type 0]
            set l [linsert [linsert [samplerange $type] 0 $val] end-5 $val]
            lappend l $val
            clequal [tarray::column search -all -eq [newspancolumn $type $l] $val] int [lsearch -exact -all $l $val]
        } -result 1

        test column_search-span-$type-eq-2.5 {
